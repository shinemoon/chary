{
	"name": "Chrary",
	"version": "1.0",
	"description": "搜索豆瓣书籍信息，并且查找本地图书馆及在线资源.",
	"background":{
		"page":"background.html"
	},
	"manifest_version": 2,
	"icons": {
	"48": "images/icon.png",
	"128": "images/icon.png"
  	},
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup":"popup.html"
    },
  	"permissions": [
		"tabs",
		"storage",
		"<all_urls>"
	]
}
